      "articulo": [
        {
          "numero-articulo": 1,
          "texto": "ARTÍCULO 1°.- Declárese Fiesta Nacional a \"La Fiesta del Puestero\" durante el mes de febrero de cada año."
        },
        {
          "numero-articulo": 2,
          "texto": "ARTÍCULO 2°.- Institúyase Sede Nacional Permanente de la Fiesta Nacional del Puestero a la ciudad de Junín de los Andes, provincia del Neuquén."
        },
        {
          "numero-articulo": 3,
          "texto": "ARTÍCULO 3°.- Inclúyase la misma en el Calendario Turístico Nacional."
        },
        {
          "numero-articulo": 4,
          "texto": "ARTÍCULO 4°.- Comuníquese al Poder Ejecutivo nacional."
        }
