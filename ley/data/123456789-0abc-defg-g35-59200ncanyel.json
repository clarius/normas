      "articulo": [
        {
          "numero-articulo": 1,
          "texto": "ARTICULO 1º - Declárase monumento histórico nacional al edificio de la Escuela Normal Superior Nº 2 Juan María Gutiérrez, ubicado en la ciudad de Rosario, provincia de Santa Fe."
        },
        {
          "referencias-normativas": {
            "referencia-normativa": [
              {
                "cr": null,
                "id": "REFERENCIAS_NORMATIVAS_0",
