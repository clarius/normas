          "modifica-a": {
            "referencia-normativa": {
              "cr": "(B.O. 3/01/2003) INCORPORA COMO ARTICULO 3",
              "id": "MODIFICA_A_0",
              "ref": "LEY C 025687 2002 11 28 0002 000 0000 000"
            }
          },
          "numero-articulo": 3,
          "texto": "ARTICULO 3 - Deróguese toda otra norma que se oponga a la presente."
        },
        {
          "numero-articulo": 4,
          "texto": "ARTICULO 4 - Comuníquese al Poder Ejecutivo."
