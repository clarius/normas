        {
          "titulo-particion": "CAPITULO IV",
          "articulo": [
            {
              "modifica-a": {
                "referencia-normativa": {
                  "cr": "SUSTITUYE EL SEGUNDO PARRAFO",
                  "id": "MODIFICA_A_0",
