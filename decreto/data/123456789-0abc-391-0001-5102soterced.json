      "articulo": [
        {
          "numero-articulo": 1,
          "texto": "Artículo 1° - Desígnase a la Doctora Paula María BERTOL (D.N.I. N° 17.255.630) en el cargo de Secretaria de Relaciones Parlamentarias y Administración de la JEFATURA DE GABINETE DE MINISTROS."
        },
        {
          "numero-articulo": 2,
          "texto": "Art. 2° - Desígnase al señor Jorge Miguel GRECCO (D.N.I. N° 14.012.555) en el cargo de Secretario de Comunicación Pública de la JEFATURA DE GABINETE DE MINISTROS."
        },
        {
          "numero-articulo": 3,
          "texto": "Art. 3° - Desígnase al señor Fulvio Valerio POMPEO (D.N.I. N° 18.449.638) en el cargo de Secretario de Asuntos Estratégicos de la JEFATURA DE GABINETE DE MINISTROS."
        },
        {
          "numero-articulo": 4,
          "texto": "Art. 4° - Comuníquese, publíquese, dése a la DIRECCIÓN NACIONAL DEL REGISTRO OFICIAL y archívese."
        }
      ],
