      "articulo": [
        {
          "numero-articulo": 1,
          "texto": "Artículo 1º - Desígnase, a partir del 10 de diciembre de 2011, Secretario de Justicia del MINISTERIO DE JUSTICIA Y DERECHOS HUMANOS al doctor Alejandro Julián ALVAREZ (D.N.I. Nº 28.963.217)."
        },
        {
          "numero-articulo": 2,
          "texto": "Art. 2º - Desígnase, a partir del 10 de diciembre de 2011, Secretario de Derechos Humanos del MINISTERIO DE JUSTICIA Y DERECHOS HUMANOS al doctor Eduardo Luis DUHALDE (D.N.I. Nº 4.300.781)."
        },
        {
          "numero-articulo": 3,
          "texto": "Art. 3º - Desígnase, a partir del 10 de diciembre de 2011, Secretario de Asuntos Registrales del MINISTERIO DE JUSTICIA Y DERECHOS HUMANOS al doctor Oscar Alberto MARTINI (D.N.I. Nº 8.345.908)."
        },
        {
          "numero-articulo": 4,
          "texto": "Art. 4º - Comuníquese, publíquese, dése a la Dirección Nacional del Registro Oficial y archívese."
        }
