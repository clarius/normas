      "articulo": [
        {
          "numero-articulo": 1,
          "texto": "Artículo 1º - Desígnase, a partir del 10 de diciembre de 2011, en el cargo de Secretario de Coordinación y Monitoreo Institucional del MINISTERIO DE DESARROLLO SOCIAL, al contador D. Carlos Daniel CASTAGNETO (D.N.I. Nº 13.942.516)."
        },
        {
          "numero-articulo": 2,
          "texto": "Art. 2º - Desígnase, a partir del 10 de diciembre de 2011, en el cargo de Secretario de Gestión y Articulación Institucional del MINISTERIO DE DESARROLLO SOCIAL, al doctor D. Juan Carlos NADALICH (D.N.I. Nº 10.353.271)."
        },
        {
          "numero-articulo": 3,
          "texto": "Art. 3º - Desígnase, a partir del 10 de diciembre de 2011, en el cargo de Secretario de Economía Social del MINISTERIO DE DESARROLLO SOCIAL, al doctor D. Carlos Sergio CIPOLLA (D.N.I. Nº 14.415.938)."
        },
        {
          "numero-articulo": 4,
          "texto": "Art. 4º - Desígnase, a partir del 10 de diciembre de 2011, en el cargo de Secretaria de Organización y Comunicación Comunitaria del MINISTERIO DE DESARROLLO SOCIAL, a la nutricionista Da. Inés del Carmen PAEZ D'ALESSANDRO (D.N.I. Nº 5.137.543)."
        },
        {
          "numero-articulo": 5,
          "texto": "Art. 5º - Desígnase, a partir del 10 de diciembre de 2011, en el cargo de Secretario de Deporte del MINISTERIO DE DESARROLLO SOCIAL, al señor D. Claudio Alberto MORRESI (D.N.I. Nº 14.872.455)."
        },
        {
          "numero-articulo": 6,
          "texto": "Art. 6º - Desígnase, a partir del 10 de diciembre de 2011, en el cargo de Secretario Nacional de Niñez, Adolescencia y Familia del MINISTERIO DE DESARROLLO SOCIAL, al doctor D. Gabriel LERNER (D.N.I. Nº 14.872.833)."
        },
        {
          "numero-articulo": 7,
          "texto": "Art. 7º - Comuníquese, publíquese, dése a la Dirección Nacional del Registro Oficial y archívese."
        }
