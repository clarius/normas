      "articulo": [
        {
          "numero-articulo": 1,
          "texto": "Artículo 1º - Desígnase, a partir del 10 de diciembre de 2011, Secretario de Gabinete y Coordinación Administrativa de la JEFATURA DE GABINETE DE MINISTROS, al Lic. D. Facundo Patricio NEJAMKIS (D.N.I. Nº 24.030.891)."
        },
        {
          "numero-articulo": 2,
          "texto": "Art. 2º - Desígnase, a partir del 10 de diciembre de 2011, Secretario de Comunicación Pública de la JEFATURA DE GABINETE DE MINISTROS, a D. Alfredo Eduardo SCOCCIMARRO (D.N.I. Nº 14.768.849)."
        },
        {
          "numero-articulo": 3,
          "texto": "Art. 3º - Desígnase, a partir del 10 de diciembre de 2011, Secretario de Ambiente y Desarrollo Sustentable de la JEFATURA DE GABINETE DE MINISTROS, al Dr. D. Juan José MUSSI (M.I. Nº 5.187.712)."
        },
        {
          "numero-articulo": 4,
          "texto": "Art. 4º - Desígnase, a partir del 10 de diciembre de 2011, Secretario de Relaciones Parlamentarias de la JEFATURA DE GABINETE DE MINISTROS, al Dr. D. Oscar GONZALEZ (D.N.I. Nº 4.704.960)."
        },
        {
          "numero-articulo": 5,
          "texto": "Art. 5º - Desígnase, a partir del 10 de diciembre de 2011, Secretario de Evaluación Presupuestaria de la JEFATURA DE GABINETE DE MINISTROS, al Lic. D. Gastón SUAREZ (D.N.I. Nº 25.250.285)."
        },
        {
          "numero-articulo": 6,
          "texto": "Art. 6º - Desígnase, a partir del 10 de diciembre de 2011, Secretaria de Integración Nacional de la JEFATURA DE GABINETE DE MINISTROS, a la señora María del Carmen ALARCON (D.N.I. Nº 11.278.455)."
        },
        {
          "numero-articulo": 7,
          "texto": "Art. 7º - Desígnase, a partir del 10 de diciembre de 2011, Jefe de Gabinete de Asesores de la JEFATURA DE GABINETE DE MINISTROS, con rango y jerarquía de Secretario, al señor D. Lucas Patricio NEJAMKIS (D.N.I. Nº 25.923.651)."
        },
        {
          "numero-articulo": 8,
          "texto": "Art. 8º - Comuníquese, publíquese, dése a la Dirección Nacional del Registro Oficial y archívese."
        }
